{
  "functions": {
    "api/getsong.php": {
      "runtime": "now-php@0.0.9"
    },
	"api/index.php": {
      "runtime": "now-php@0.0.9"
    },
    "api/search.php": {
      "runtime": "now-php@0.0.9"
    }
  },
  "routes": [
    { "src": "/(?<name>[^/]+)",  "dest": "/api/getsong.php?station=$name" },
	{ "src": "/",  "dest": "/api/index.php" },
    { "src": "/search/(?<name>[^/]+)",  "dest": "/api/search.php?input=$name" }
  ]
}